import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;
import java.util.ArrayList;
public class OpenAiOutputReader {
    String rP;
    String PassKeyStart = "<<<";
    String PassKeyEnd = ">>>";
    File file;
    public static void main(String[] args) {
        File f = new File("my.txt");
        OpenAiOutputReader m = new OpenAiOutputReader(f);
    }
    
    public OpenAiOutputReader(File Response) {
        file = Response;
        String Answer;
        try {
            Answer = ReadFile();    
        } catch (FileNotFoundException e) { System.out.println("file aint found"); return; }

        System.out.println(Answer);
        System.out.println("Program exited with exit code 69");
    }

    public String ReadFile() throws FileNotFoundException {
		Scanner sc = new Scanner(file);
		ArrayList <String> arr =  new ArrayList<>();
        int index = 0;
        int index1 = -1;
        String Line = "";
		while(sc.hasNext() && index1 == -1) {
            Line = sc.nextLine();
            index1 = ( Line.indexOf(PassKeyStart) != -1 ) ? Line.indexOf(PassKeyStart) : -1;
	    }
        index+=PassKeyStart.length();

        int index2;
        if (index1<Line.length() ) {
            index2 = Line.indexOf(PassKeyEnd, index1);
        } else { System.out.println("FATAL ERROR Line never initialized"); return null; }
        
        if (index2 == -1) { System.out.println("FATAL ERROR ADKLSFJS;LADFJDS;LKJ"); return null; }
        return Line.substring(index1+PassKeyStart.length(),index2);
    }

    
}